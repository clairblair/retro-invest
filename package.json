{
  "name": "investment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" --names \"frontend,backend\" --prefix name --kill-others-on-fail",
    "dev:frontend": "next dev --turbopack",
    "dev:backend": "nest start --watch",
    "dev:frontend-only": "next dev --turbopack",
    "dev:backend-only": "nest start --watch",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "next build",
    "build:backend": "nest build",
    "build:all": "npm run build:frontend && npm run build:backend",
    "start": "next start",
    "start:backend": "nest start",
    "start:prod": "node dist/src/main",
    "start:production": "concurrently \"pnpm start\" \"cd backend && pnpm run start:prod\" --names \"frontend,backend\" --prefix name --kill-others-on-fail",
    "lint": "next lint",
    "lint:backend": "eslint \"{backend/src,backend/apps,backend/libs,backend/test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./backend/test/jest-e2e.json",
    "seed": "ts-node -r tsconfig-paths/register backend/src/seeds/seed.ts",
    "seed:clear": "npm run seed clear",
    "seed:plans": "npm run seed plans",
    "seed:admin": "npm run seed admin",
    "seed:all": "npm run seed seed",
    "seed:reset": "npm run seed reset"
  },
